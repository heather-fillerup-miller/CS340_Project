<table>
    <thead>
        <tr>
            <th class= "title" colspan={{columnNumber}}>Mahinui Auto Shop {{tableName}}</th>
        </tr>
        {{#if columnNames}}
        <tr>
            {{#each columnNames}}
              <th>{{this}}</th>
            {{/each}}
            <th colspan= "2">
              <form class="form-inline">
                <div class="input-group">
                    <input type="text" name="filter" id="filter">
                    <button id="search" type="button">Search</button>
                </div>
              </form>
            </th>
        </tr>
        {{/if}}
    </thead>

    {{#if dataResults}}
    <tbody>
      {{#each dataResults}}
			  <tr id= "{{this}}">
  			  <td style= "border: 1px solid black">{{this}}</td>
          <td type= "button"><button id= "{{this}}Update">Update</button></td>
          <td type= "button"><button id= "{{this}}Delete">Delete</button></td>
			  </tr>
			{{/each}}
    </tbody>
    {{/if}}

</table>
<br>
